<script>

$(window).on("load", function(){
	fncGetList("1", "N");
});


function fncKeyEvent(){
    if(event.keyCode == 13){
    	fncGetList("1", "N");
    }
}


function fncGoView(bbsSn){
    jQuery("#bbsSn").val(bbsSn);
    jQuery("#_method").val("view");
    jQuery("#form").attr("action", "/membership/event.do");
    jQuery("#form").attr("method", "get");
    jQuery("#form").submit();	

}

function fncFileDownload(sn, code){
	$("#fileSn").val(sn);
	$("#sysCode").val(code);
	$("#fileForm").attr("method", "get");
	$("#fileForm").attr("action", "/util/file/download.do");
	$("#fileForm").submit();
}

function fncGetList(page, isMore){

	$("#_method").val("list");
	$("#page").val(page);
	var param = $("#form").serialize();

	jQuery.ajax({
		type : "POST",
		url : "/membership/event.json",
		chache : false,
		dataType : "json",
		data : param,
		global : false,
		beforeSend : function() {
			commonJs.showLoadingBar();
			if(isMore == "N"){
				$("#listArea").html("");
				$("#moreArea").hide();
			}
        },
		success : function(data){

			var html = "";

			var totalCnt = data.totalCount;
			var pagingRow = data.bbsMstr.pagingRecordCo;
			var pageCnt = parseInt(totalCnt)/parseInt(pagingRow);
			var sysCode = data.vo.sysCode;
			var list = data.list;

			removeItemByDateField(list, "eventStartDt","eventEndDt");
console.log("boki===="+list);
			if(list.length > 0){

				for(var i = 0; i < list.length; i++){

					html += "<li>";
					html += "	<dl>";
					html += "		<dt>"+list[i].title+"</dt>";

					if(list[i].files.length > 0){
						for(var j = 0; j < list[i].files.length; j++){
							if(j == 0){
								html += "<dd class=\"thum\">";
								html += "	<img src=\"/util/file/download.do?fileSn="+list[i].files[j].fileSn+"&sysCode="+sysCode+"\" alt=\""+list[i].files[j].fileNm+"\"/>";
								html += "</dd>";
							}
						}
					}


					html += "	<dd class=\"locationTag "+list[i].ctgry+"\">";
					html +=			list[i].hotelNm;
					html += "	</dd>";
					html += "	<dd class=\"txt\">";
					if(list[i].addFieldList.length > 0){
						for(var k = 0; k < list[i].addFieldList.length; k++){
							if(list[i].addFieldList[k].fieldId == "contentDc" && list[i].addFieldList[k].fieldContents != "" && list[i].addFieldList[k].fieldContents != null){
								html += list[i].addFieldList[k].fieldContents;
							}
						}
					}
					html += "	</dd>";

					html += "	<dd class=\"date\">";

					var eventSttDate = "";
					var eventEndDate = "";
					var eventTarget = "";
                    var detailViewLinkUrl = "";

					if(list[i].addFieldList.length > 0){
						for(var k = 0; k < list[i].addFieldList.length; k++){
							if(list[i].addFieldList[k].fieldId == "eventStartDt" && list[i].addFieldList[k].fieldContents != ""){
								eventSttDate = list[i].addFieldList[k].fieldContents;
							}
							if(list[i].addFieldList[k].fieldId == "eventEndDt" && list[i].addFieldList[k].fieldContents != ""){
								eventEndDate = list[i].addFieldList[k].fieldContents;
							}
							if(list[i].addFieldList[k].fieldId == "target" && list[i].addFieldList[k].fieldContents != ""){
								eventTarget = list[i].addFieldList[k].fieldContents;
							}
                            if(list[i].addFieldList[k].fieldId == "detailViewLinkUrl" && list[i].addFieldList[k].fieldContents != ""){
								detailViewLinkUrl = list[i].addFieldList[k].fieldContents;
							}
						}
					}

					if(eventSttDate != "" && eventEndDate != ""){
						html += "	<div>"+eventSttDate+" - "+eventEndDate+"</div>";
					}

					if(eventTarget != ""){
						html += "	<div>"+eventTarget+"</div>";
					}

					html += "		</dd>";
					html += "	</dl>";
					html += "	<div class=\"btn\">";
					var detailViewTxt = '자세히 보기';
                    if(detailViewLinkUrl){
                        html += "		<a href='"+detailViewLinkUrl+"' class=\"btnSC btnM active\" >"+detailViewTxt+"</a>";
                    }else {
                        html += "		<a href=\"#none\" class=\"btnSC btnM active\" onclick=\"fncGoView('"+list[i].bbsSn+"');\">"+detailViewTxt+"</a>";
                    }

					html += "	</div>";
					html += "</li>";
				}

				$("#listArea").append(html);

				commonJs.setParallax($('.parallaxUp'));

				if(page < pageCnt){
					$("#moreArea").html("<a href=\"#none\" class=\"btnSC btnMore\" onclick=\"fncGetList('"+(parseInt(page)+1)+"', 'Y');\">MORE</a>");
					$("#moreArea").show();
				}else{
					$("#moreArea").hide();
				}

			}else{
				var nodataTxt = '등록된 이벤트 상품이 없습니다.'
				$("#listArea").html("<li class=\"noData\"><p class=\"txt\">"+nodataTxt+"</p></li>");
			}
			commonJs.closeLoadingBar();

		},
		error:function(r, s, e){
			alert('Ajax 통신중 에러가 발생하였습니다\nError Code : \"{1}\"\nError : \"{2}\"'.replace("{1}", r.status).replace("{2}", r.responseText));
			commonJs.closeLoadingBar();
			return false;
		}
	});
}

</script>

