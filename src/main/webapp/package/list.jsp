							}
							for(var j=0; j<kwrdListLength; j++){
								html += '		<span>'+kwrdList[j].kwrdNm +'</span>';
							}
							html += '		</dd>';
							html += '		<dd class="txt">'+fncReplaceEnter(packageDc)+'</dd>';
							html += '		<dd class="date">';
							html += '			<div>';
							html +=	'			<span>';
							if(sleDeFieldNm != "" && sleDeFieldNm != null){
