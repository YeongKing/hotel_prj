	// 검색
	function fncSearch() {
		fncPage("1");
	}
	
	function fncPage(page){
		
		var searchDataBeginDe = jQuery("#datepickerFrom").val();	// 검색시작일
		var searchDataEndDe = jQuery("#datepickerTo").val();		// 검색종료일
		
		// 시작일자만 입력되었을 경우
		if (searchDataBeginDe != "" && searchDataEndDe == "") {
			alert("검색 종료일을 입력해 주세요.");	//검색 종료일을 입력해 주세요.
			jQuery("#datepickerFrom").focus();
			return;
		}
		
		// 종료일자만 입력되었을 경우
		if (searchDataBeginDe == "" && searchDataEndDe != "") {
			alert("검색 시작일을 입력해 주세요."); //검색 시작일을 입력해 주세요.
			jQuery("#datepickerTo").focus();
			return;
		}
		
		// 검색 시작일자가 종료일자보다 큰경우
		if (searchDataBeginDe > searchDataEndDe){
			alert("검색 시작일이 종료일보다 큽니다.");			//검색 시작일이 종료일보다 큽니다.
			jQuery("#datepickerTo").focus();
			return;
		}
		
	    jQuery("#searchPage").val(page);
	    jQuery("#searchForm").attr("action", "/package/list.do");
	    jQuery("#searchForm").attr("method", "get");
	    jQuery("#searchForm").submit();	//검색 시 true값으로 설정-validation 체크를 하지 않음 
	    
	}
	
	
	function fncAddInterest(packageSn, _this, sysCode){

        var $li = jQuery(_this).closest("li[data-id]");

        var name = $li.data("name");
        var index = $li.data("index");

        //2022-12-23 추가 , 관심 상품 추가시
        gfncPushInterestAddSeoData(packageSn, name, sysCode, index);
		
		jQuery.ajax({
			type : "GET",
			url : "/mber/interest/addPackageInterest.json",
			cache : false,
			dataType : "json",
			data : {
				"packageSn" : packageSn,
				"sysCode" : sysCode
			},
			global : false,
			beforeSend: function() {
				commonJs.showLoadingBar(); //로딩바 show
         	},
            complete: function() {
				commonJs.closeLoadingBar(); //로딩바 hide
            },
			success : function(data){
				
				//비 로그인 시
				var resultCode = data.resultCode;
				var resultMsg = data.resultMsg;
				if(resultCode == "SUCCESS"){
					alert('나의 관심 상품이 추가 되었습니다.'); /* 나의 관심 상품이 추가되었습니다. */
					jQuery(_this).attr("onclick", "fncDelInterest('"+packageSn+"', this, '"+sysCode+"')");
					jQuery(_this).prop("checked", true);
				}else{
