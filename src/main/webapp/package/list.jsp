							html += '		<dd class="thum"><img src="/util/file/download.do?fileSn='+thumbSn +'&sysCode='+packageSysCode+'" alt="'+thumbDc+'"></dd>';
							html += '		<dd class="keyword">';
							
							if(sysCode === 'JOSUNHOTEL'){
								html += 	'<span class="locationTag '+packageSysCode+'">'+hotelNm+'</span>';
							}
							for(var j=0; j<kwrdListLength; j++){
								html += '		<span>'+kwrdList[j].kwrdNm +'</span>';
							}
							html += '		</dd>';
							html += '		<dd class="txt">'+fncReplaceEnter(subtitle)+'</dd>';
							html += '		<dd class="date">';
							html += '			<div>';
							html +=	'			<span>';
							if(sleDeFieldNm != "" && sleDeFieldNm != null){
								html += sleDeFieldNm
							}else {
								html += '		예약 기간';
							}
							html += '			</span>';
							html += '			<em>'+gfncDateFormat(sleBeginDe, 'yyyy.MM.dd')+' - '+gfncDateFormat(sleEndDe, 'yyyy.MM.dd')+'</em>';
							html += '			</div>';
							html += '			<div>';
							html +=	'			<span>';
							if(stayDeFieldNm != "" && stayDeFieldNm != null){
