    /**
     * 이메일 형식 체크
     * @param str    검증 문자열
     * @returns {Boolean}
     */
    function gfncEmailCheck(str) {
        var regExp = /[0-9a-zA-Z][_0-9a-zA-Z-]*@[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+){1,2}$/;

        //입력을 안했으면
        if (str.lenght == 0) {
            return false;
        }

        //이메일 형식에 맞지않으면
        if (str.match(regExp)) {
            return true;
        }

        return false;
    }

